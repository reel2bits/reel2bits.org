{
  "name": "reel2bits.org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "scripts/i18n-compile.sh && vue-cli-service serve",
    "build": "scripts/i18n-compile.sh && vue-cli-service build",
    "i18n-compile": "scripts/i18n-compile.sh",
    "i18n-extract": "scripts/i18n-extract.sh",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.3",
    "fork-awesome": "^1.1.7",
    "lodash": "^4.17.11",
    "marked": "^0.7.0",
    "register-service-worker": "^1.6.2",
    "typeface-montserrat": "^0.0.75",
    "typeface-noto-sans": "^0.0.72",
    "vue": "^2.6.6",
    "vue-gettext": "^2.1.2",
    "vue-head": "^2.1.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "easygettext": "^2.7.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "prerender-spa-plugin": "^3.4.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "resolutions": {
    "webpack": "4.39"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
